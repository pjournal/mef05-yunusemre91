convert_temperature <- function(value,F_to_C=FALSE){
if(F_to_C){
return_value <- value*9/5+32
}else{
return_value <- (value - 32)*5/9
}
return(return_value)
}
convert_temperature(212,F_to_C = FALSE)
convert_temperature(212,F_to_C = TRUE)
convert_temperature(100, F_to_C = FALSE)
convert_temperature(100, F_to_C = TURE)
convert_temperature(100, F_to_C = TRUE)
